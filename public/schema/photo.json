    "title": "活动页",
    "type": "array",
    "format": "tab",   
    "options": {
        "disable_collapse": true
    },
    "items": {

        "type": "object",
        "headerTemplate": "商品{{ i1 }} - {{ self.twitter }}",
        "properties": {
            "twitter": {
                "type": "string"
            },
            "image": {
                "type": "string",
                "links": [{
                    "href": "{{self}}",
                    "mediaType": "image"
                }]
            },
            "link": {
                "type": "string"
            }

        }

    },
